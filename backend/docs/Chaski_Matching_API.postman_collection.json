            "url": {
              "raw": "{{baseUrl}}/api/packages/{{packageId}}",
              "host": ["{{baseUrl}}"],
              "path": ["api", "packages", "{{packageId}}"]
            }
          }
        },
        {
          "name": "Update Status - Picked Up",
          "request": {
            "method": "PUT",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              },
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"status\": \"picked_up\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/packages/{{packageId}}/status",
              "host": ["{{baseUrl}}"],
              "path": ["api", "packages", "{{packageId}}", "status"]
            }
          }
        },
        {
          "name": "Update Status - In Transit",
          "request": {
            "method": "PUT",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              },
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"status\": \"in_transit\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/packages/{{packageId}}/status",
              "host": ["{{baseUrl}}"],
              "path": ["api", "packages", "{{packageId}}", "status"]
            }
          }
        },
        {
          "name": "Update Status - Delivered",
          "request": {
            "method": "PUT",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{token}}"
              },
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"status\": \"delivered\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/packages/{{packageId}}/status",
              "host": ["{{baseUrl}}"],
              "path": ["api", "packages", "{{packageId}}", "status"]
            }
          }
        }
      ]
    },
